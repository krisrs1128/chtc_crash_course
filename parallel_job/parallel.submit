universe=docker
docker_image=rstudio/rstudio:latest

executable=basic.sh
transfer_input_files=configurations.csv

# requesting resources
request_memory=10MB
request_cpus=1
request_disk=1MB

# parameters
environment = "id=$(Step)"

# log files
error=basic-$(Process).err
output=basic-$(Process).out
log=basic-$(Process).log
queue 5
